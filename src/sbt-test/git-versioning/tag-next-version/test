> git init
> git config user.email "test@jsuereth.com"
> git config user.name "Tester"
> git add README.md
$ copy-file changes/build.sbt build.sbt
> git add build.sbt
> git commit -m "test"
> git tag v1.0.0
> git checkout -b bugfix/branch
> git add README2.md
> git commit -m "test2"
> git checkout master
> git merge --no-ff --no-edit bugfix/branch
> reload
> tagNextVersion
> reload
> checkTaggedFix
> git checkout -b feature/branch
> git add README3.md
> git commit -m "test2"
> git checkout master
> git merge --no-ff --no-edit feature/branch
> reload
> tagNextVersion
> reload
> checkTaggedFeature
> git checkout -b major/branch
> git rm README3.md
> git rm README2.md
> git commit -m "test2"
> git checkout master
> git merge --no-ff --no-edit major/branch
> reload
> tagNextVersion
> reload
> checkTaggedMajor